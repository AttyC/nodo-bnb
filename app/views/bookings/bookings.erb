<table id="bookings">
      <caption>My requests</caption>
      <tr>
        <th>Name</th>
        <th>Approve</th>
      </tr>
      <% @space.each do |space| %>
      <tr>
        <td><%= space.name %></td>
         <td><form action="/bookings" method="post">
          <input type="hidden" name="_method" value="patch" />
          <input type="submit" value="Approve" id='space<%= space.id %>' >
        </form> </td>
      </tr>
      <% end %>
</table>
